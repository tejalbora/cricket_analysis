              "          const element = document.querySelector('#df-56dff5c7-c4df-4579-9993-ffea6d893aa5');\n",
              "          const dataTable =\n",
              "            await google.colab.kernel.invokeFunction('convertToInteractive',\n",
              "                                                     [key], {});\n",
              "          if (!dataTable) return;\n",
              "\n",
